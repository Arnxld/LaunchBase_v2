{% extends "layout.njk"%}


{% block head %}
<title>Mayk Brito</title>
{% endblock %}


{% block content %}
<div id="wrapper">
    <img src="{{about.image}}" alt="{{about.name}}">
    <h1>{{about.name}}</h1>
    <h2>{{about.role}}</h2>
    <p>{{about.description}}</p>
</div>

<div id="links-footer" class="links">
    <a href="{{about.links.github}}">Github</a>
    <a href="{{about.links.instagram}}">Instagram</a>
    <a href="{{about.links.facebook}}">Facebook</a>
</div>
{% endblock %}